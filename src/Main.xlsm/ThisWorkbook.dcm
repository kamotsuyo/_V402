VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "ThisWorkbook"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Option Explicit

'*************************
'Main ThisWorkbook
'2025/12/12
'*************************

Private WithEvents SubMan As SubProject.MM_SubManager
Attribute SubMan.VB_VarHelpID = -1

Private Sub Workbook_Open()

    Me.Windows.Application.Width = 500
    Me.Windows.Application.Height = 500
    
    Set SubMan = SubProject.SubManager
    
End Sub

Private Sub Workbook_BeforeClose(Cancel As Boolean)
    Call DeleteSheetWithoutSheet1
End Sub

'-----------------
'SubProject
'-----------------
Private Sub SubMan_CodeResolved()

    FormLog.Show vbModeless
    F_Strategys.Show vbModeless
    
    
    '-----------------
    'メイン側プログラム開始
    '-----------------
    Call MainModule.Start
End Sub

Private Sub SubMan_ConnctChange(Status_ As String)
    Debug.Print "Man_ConnctChange", Status_
End Sub
